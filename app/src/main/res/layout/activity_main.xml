<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/img0"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/img1"
        app:layout_constraintHeight_percent="0.3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.18"
        app:srcCompat="@drawable/zero" />

    <ImageView
        android:id="@+id/img1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@+id/img0"
        app:layout_constraintStart_toEndOf="@id/img0"
        app:layout_constraintTop_toTopOf="@id/img0"
        app:layout_constraintHeight_percent="0.3"
        app:layout_constraintWidth_percent="0.18"
        app:srcCompat="@drawable/one" />

    <ImageView
        android:id="@+id/img2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@id/img1"
        app:layout_constraintStart_toEndOf="@id/img1"
        app:layout_constraintTop_toTopOf="@id/img1"
        app:layout_constraintHeight_percent="0.3"
        app:layout_constraintWidth_percent="0.18"
        app:srcCompat="@drawable/two" />

    <ImageView
        android:id="@+id/img3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@id/img2"
        app:layout_constraintStart_toEndOf="@id/img2"
        app:layout_constraintTop_toTopOf="@id/img2"
        app:layout_constraintHeight_percent="0.3"
        app:layout_constraintWidth_percent="0.18"
        app:srcCompat="@drawable/three" />
    <ImageView
        android:id="@+id/img4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@id/img3"
        app:layout_constraintStart_toEndOf="@id/img3"
        app:layout_constraintTop_toTopOf="@id/img3"
        app:layout_constraintHeight_percent="0.3"
        app:layout_constraintWidth_percent="0.18"
        app:srcCompat="@drawable/four" />

    <ImageView
        android:id="@+id/img5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@id/img0"
        app:layout_constraintStart_toStartOf="@id/img0"
        app:layout_constraintHeight_percent="0.3"
        app:layout_constraintWidth_percent="0.18"
        app:srcCompat="@drawable/five" />

    <ImageView
        android:id="@+id/img6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@+id/img5"
        app:layout_constraintStart_toEndOf="@id/img5"
        app:layout_constraintTop_toTopOf="@id/img5"
        app:layout_constraintHeight_percent="0.3"
        app:layout_constraintWidth_percent="0.18"
        app:srcCompat="@drawable/six" />

    <ImageView
        android:id="@+id/img7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@+id/img6"
        app:layout_constraintStart_toEndOf="@id/img6"
        app:layout_constraintTop_toTopOf="@id/img6"
        app:layout_constraintHeight_percent="0.3"
        app:layout_constraintWidth_percent="0.18"
        app:srcCompat="@drawable/seven" />

    <ImageView
        android:id="@+id/img8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@+id/img7"
        app:layout_constraintStart_toEndOf="@id/img7"
        app:layout_constraintTop_toTopOf="@id/img7"
        app:layout_constraintHeight_percent="0.3"
        app:layout_constraintWidth_percent="0.18"
        app:srcCompat="@drawable/eight" />

    <ImageView
        android:id="@+id/img9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@+id/img8"
        app:layout_constraintStart_toEndOf="@id/img8"
        app:layout_constraintTop_toTopOf="@id/img8"
        app:layout_constraintHeight_percent="0.3"
        app:layout_constraintWidth_percent="0.18"
        app:srcCompat="@drawable/nine" />

    <ImageView
        android:id="@+id/img10"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.3"
        app:layout_constraintStart_toStartOf="@id/img7"
        app:layout_constraintTop_toBottomOf="@id/img7"
        app:layout_constraintWidth_percent="0.18"
        app:srcCompat="@drawable/ten" />

    <ImageView
        android:id="@+id/quit"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/img9"
        app:layout_constraintWidth_percent="0.1"
        app:srcCompat="@drawable/quit" />


</androidx.constraintlayout.widget.ConstraintLayout>